<h1>yolo</h1>
<h2>Sign in</h2>
<%= form_for @user, url: {action: "login"} do |f| %>
  <%= f.label(:email) %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Sign in" %>
<% end %>

<h5><%= link_to("Create new account", new_user_path) %></h5>
